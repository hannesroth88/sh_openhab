	        	{	        		
			        Text item=EG_Schlafen_Raumtemperatur_MW label="Raumtemperatur Ist [%.1f °C]"
			        Setpoint item=EG_Schlafen_Raumtemperatur_SW label="Raumtemperatur Soll [%.1f °C]" icon="none" step=0.5 minValue=14 maxValue=30
