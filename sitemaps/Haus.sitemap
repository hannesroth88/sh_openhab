				Slider item=EG_Wohnen_LG3_Dim label="Licht Sofa [%d %%]" icon="light" 
	        	Switch item=EG_Wohnen_Esstischlampe label="Esstischlampe" icon="light"  mappings=[ON="An", OFF="Aus"]
