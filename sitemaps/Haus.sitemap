sitemap Haus label="Hauptmenü"
{
    Frame label="Räume"{
    				
	        Group item=gEG_Wohnen label="Wohnnzimmer"
	        {
				Slider item=EG_Wohnen_LG1_Dim label="Licht Tisch [%d %%]" icon="light" 
