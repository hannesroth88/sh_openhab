	        }
								
}


Frame label="Zentral" {          		
	        	Switch item=EG_Zentral_Beleuchtung label="Gesamte Beleuchtung" icon="light" mappings=[OFF="Aus"]  
	        	Switch item=EG_Zentral_Rolladen 
	        	Switch item=EG_Fassade_Lager
	        	Switch item=EG_Fassade_Terrasse
	        	Switch item=EG_Fassade_Strasse
}
Frame label="Wetter" {        		
	        	Webview url="/weather?locationId=home&layout=example&iconset=colorful" height=8 icon=none
	        	Text item=Astro_Sunrise_Time
	        	Text item=Astro_Sunset_Time
	        	Text item=Azimuth
	        	Text item=Elevation
}

Frame label="Allgemein"{
	        	//Group item=gWeather label="Wetter"
	        	Group item=gHeizung label="Heizung"
				Group item=gFenster label="Fenster"		
				Group item=gEG_Rolladen label="Rolladen"{
			  	  Frame label="Status" {
			        Text item=EG_Buero_Rolladen1 label="Büro Terrasse [%d %%]"
			        Text item=EG_Buero_Rolladen2 label="Büro Strasse [%d %%]"
			        Text item=EG_Schlafen_Rolladen1 label="Schlafen [%d %%]"
			        Text item=EG_Kueche_Rolladen1 label="Küche [%d %%]"
			        Text item=EG_Wohnen_Rolladen1 label="Wohnen Fenster [%d %%]"
			        Text item=EG_Wohnen_Rolladen2 label="Wohnen Tür [%d %%]"
			        Text item=EG_Bad_Rolladen1 label="Bad [%d %%]"
					}
			        
			  	  Frame label="Einstellungen" {
					/*-----------Konfiguration Rolladen Hitzeschutz-----------*/
					Switch item= EG_Rolloautomatik  mappings=[ON="An", OFF="Aus"]
					//Switch item= EG_Hitzeschutz visibility=[EG_Rolloautomatik=="ON"] //wird nun von der Regel gesetzt
					Text item= EG_Hitzeschutz icon="sun" //wird nun von der Regel gesetzt
					Setpoint item= EG_Hitzeschutz_SW_MaxTemp minValue=20 maxValue=40 step=1
					Setpoint item= EG_SW_Azimuth_Fassade_LagerAb minValue=0 maxValue=360 step=10
					Setpoint item= EG_SW_Azimuth_Fassade_LagerAuf minValue=0 maxValue=360 step=10
					Setpoint item= EG_SW_Azimuth_Fassade_TerrasseAb minValue=0 maxValue=360 step=10
					Setpoint item= EG_SW_Azimuth_Fassade_StrasseAb minValue=0 maxValue=360 step=10
			      }
			  	  Frame label="letzte Fahrten" {
					Text item= EG_Rolloautomatik_Zentral_LastAuf icon="calendar" label="Rolladen Sonnenaufgang [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
					Text item= EG_Rolloautomatik_Zentral_LastAb icon="calendar" label="Rolladen Sonnenuntergang [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
					Text item= EG_Rolloautomatik_Lager_LastAb icon="calendar" label="Rolladen Ab Lager [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
					Text item= EG_Rolloautomatik_Terrasse_LastAb icon="calendar" label="Rolladen Ab Terrasse [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
					Text item= EG_Rolloautomatik_Strasse_LastAb icon="calendar" label="Rolladen Ab Strasse [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
					Text item= EG_Rolloautomatik_Lager_LastAuf icon="calendar" label="Rolladen Auf Lager [%1$ta, %1$tB  %1$td, %1$tY - %1$tI:%1$tM %1$Tp]"
			      }
			      }
				Group item=gLog label="Logger" icon="text"{
			      Webview url="http://192.168.2.4:9001/" height=500  icon=none
			      }
			      
			      		
}

} //end of sitemap
